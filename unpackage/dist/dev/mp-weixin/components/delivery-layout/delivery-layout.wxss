@charset "UTF-8";
/* 颜色变量 */
/**
 * 自定义颜色
 */
.delivery .headNav {
  width: 100%;
  height: 78rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  justify-content: center;
  align-items: center;
  color: #EC544F;
  border-radius: 15rpx 15rpx 0 0;
}
.delivery .body {
  background: #fff;
  padding: 25rpx;
}
.delivery .body .box {
  padding: 40rpx 20rpx;
  background: #F7F7F7;
  border-radius: 10rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.delivery .body .box .center {
  flex: 1;
  padding: 0 30rpx;
  font-size: 30rpx;
}
.delivery .body .box .center .userinfo {
  padding-top: 10rpx;
}

