@charset "UTF-8";
/* 颜色变量 */
/**
 * 自定义颜色
 */
.category {
  padding: 30rpx;
}
.category .row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #efefef;
  padding: 26rpx 0;
}
.category .row .left {
  font-size: 34rpx;
}
.category .row .right {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.category .row .right .u-icon {
  margin-left: 30rpx;
}
.category .row.add .left {
  color: #576b95;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.category .row.add .left .text {
  font-size: 36rpx;
  padding-left: 10rpx;
}

